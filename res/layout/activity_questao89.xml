<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
   android:layout_height="match_parent">
  <LinearLayout  android:layout_width="fill_parent"
     android:layout_height="fill_parent"
     android:orientation="vertical">

       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/Questao89" />
    
        <CheckBox android:id="@+id/checkbox_questao89A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao89A"
        />
    
    <CheckBox android:id="@+id/checkbox_questao89B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao89B"
        />
   
    <CheckBox android:id="@+id/checkbox_questao89C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao89C"
        />
   
    <CheckBox android:id="@+id/checkbox_questao89D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao89D"
        />
    
    <CheckBox android:id="@+id/checkbox_questao89E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao89E"
        />

     <CheckBox android:id="@+id/checkbox_questao89F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao89F"
        />
    
    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left|center_vertical"
        android:text="@string/botaoQuestao"
        android:onClick="abrirResposta"/>
    
    <TextView
        android:id="@+id/RespostaQuestao89"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/RespostaQuestao89"
        android:visibility="invisible"
        />

</LinearLayout>
</ScrollView>