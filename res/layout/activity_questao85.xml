<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
   android:layout_height="match_parent">
 <LinearLayout  android:layout_width="fill_parent"
     android:layout_height="fill_parent"
     android:orientation="vertical">

       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/Questao85" />
    
        <CheckBox android:id="@+id/checkbox_questao85A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao85A"
        />
    
    <CheckBox android:id="@+id/checkbox_questao85B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao85B"
        />
   
    <CheckBox android:id="@+id/checkbox_questao85C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao85C"
        />
   
    <CheckBox android:id="@+id/checkbox_questao85D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao85D"
        />

    <CheckBox android:id="@+id/checkbox_questao85E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao85E"
        />
    
      <CheckBox android:id="@+id/checkbox_questao85F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao85F"
        />
      
    
    
    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left|center_vertical"
        android:text="@string/botaoQuestao"
        android:onClick="abrirResposta"/>
    
  
    <TextView
        android:id="@+id/RespostaQuestao85"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/RespostaQuestao85"
        android:visibility="invisible"
        />

</LinearLayout>
</ScrollView>