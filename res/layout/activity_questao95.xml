<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
   android:layout_height="match_parent">
  <LinearLayout  android:layout_width="fill_parent"
     android:layout_height="fill_parent"
     android:orientation="vertical">

       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/Questao95" />
    
        <CheckBox android:id="@+id/checkbox_questao95A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao95A"
        />
    
    <CheckBox android:id="@+id/checkbox_questao95B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao95B"
        />
   
    <CheckBox android:id="@+id/checkbox_questao95C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao95C"
        />
   
    <CheckBox android:id="@+id/checkbox_questao95D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao95D"
        />
    
    <CheckBox android:id="@+id/checkbox_questao95E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao95E"
        />

    
    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left|center_vertical"
        android:text="@string/botaoQuestao"
        android:onClick="abrirResposta"/>
    
    <TextView
        android:id="@+id/RespostaQuestao95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/RespostaQuestao95"
        android:visibility="invisible"
        />

</LinearLayout>
</ScrollView>