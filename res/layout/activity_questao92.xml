<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
   android:layout_height="match_parent">
  <LinearLayout  android:layout_width="fill_parent"
     android:layout_height="fill_parent"
     android:orientation="vertical">

       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="@string/Questao92" />
    
        <CheckBox android:id="@+id/checkbox_questao92A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92A"
        />
    
    <CheckBox android:id="@+id/checkbox_questao92B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92B"
        />
   
    <CheckBox android:id="@+id/checkbox_questao92C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92C"
        />
   
    <CheckBox android:id="@+id/checkbox_questao92D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92D"
        />
    
    <CheckBox android:id="@+id/checkbox_questao92E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92E"
        />
    
    <CheckBox android:id="@+id/checkbox_questao92F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92F"
        />
    
    <CheckBox android:id="@+id/checkbox_questao92G"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questao92G"
        />
    
    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left|center_vertical"
        android:text="@string/botaoQuestao"
        android:onClick="abrirResposta"/>
    
    <TextView
        android:id="@+id/RespostaQuestao92"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/RespostaQuestao92"
        android:visibility="invisible"
        />

</LinearLayout>
</ScrollView>